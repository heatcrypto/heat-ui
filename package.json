  },
  "build": {
    "appId": "com.heatledger.heatwallet",
    "copyright": "Copyright Â© 2018 Heat Ledger Ltd.",
    "productName": "Heatwallet",
    "compression": "normal",
    "extraResources": [
      {
        "from": "../heatledger/build/install/heatledger",
        "to": "heatledger"
      },
      {
        "from": "./dist/known-servers-config.json",
        "to": "known-servers-config.json"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "zip"
      ]
    },
    "mac": {
      "icon": "app/electron/icon.png.icns",
      "category": "public.app-category.finance",
      "target": [
        "zip"
      ]
    },
    "directories": {
      "output": "releases",
      "app": "dist"
    }
  }
